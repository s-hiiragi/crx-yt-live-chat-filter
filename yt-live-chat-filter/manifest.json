{
    "manifest_version" : 3,
    "name" : "YouTube Live Chat Filter",
    "description" : "",
    "version" : "0.1.0",
    
    "host_permissions": [
        "https://www.youtube.com/*"
    ],

    "content_scripts" : [
        {
            "all_frames" : true,
            "js" : ["content_script.js"],
            "matches" : ["https://www.youtube.com/live_chat*"]
        }
    ]
}
